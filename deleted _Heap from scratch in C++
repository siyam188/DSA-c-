#include <bits/stdc++.h>
using namespace std;
void insert_heap(vector<int> &a, int val)
{
    a.push_back(val);
    int cur_idx = a.size() - 1;

    while (cur_idx != 0) // 0 hole stop hobe
    {
        int par_idx = (cur_idx - 1) / 2;

        if (a[par_idx] > a[cur_idx])
        {
            swap(a[par_idx], a[cur_idx]);
        }
        else
        {
            break;
        }
        cur_idx = par_idx;
    }
}

    void print_heap(vector<int> a)
    {
        for (int x : a)
        {
            cout << x << " ";
        }
    }

    void delete_heap(vector<int> & a)
    {
        cout<<a[0]<<"Delete -> ";
        a[0] = a.back();
         a.pop_back();

          int cur_idx = 0;
          while (true)
          {
           int left_idx = cur_idx *2 +1;
          int right_idx = cur_idx *2 +2;
          int left_val,right_val;
          if(left_idx<a.size())
          {
            left_val = a[left_idx];
          }
          else
          {
                left_val = INT_MIN;
          }
          if(right_idx<a.size())
          {
            right_val = a[right_idx];
          }
          else
          {
               right_val = INT_MIN;
          }

          if(left_val>right_val && left_val > a[cur_idx])
          {
                swap(a[left_idx],a[cur_idx]);
                cur_idx = left_idx;
          }
          else if (right_val>left_val && right_val > a[cur_idx])
          {
                swap(a[right_idx],a[cur_idx]);
                cur_idx = right_idx;
          }
          else
          {
               break;
          }

          }
          
    }

int main()
{
    vector<int> a;
    int n;
    cin >> n;
    for (int i = 0; i < n; i++)
    {
        int val;
        cin >> val;
        insert_heap(a, val);
    }
   
    delete_heap(a);
     print_heap(a);

    return 0;
}
