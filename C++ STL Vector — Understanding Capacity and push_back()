#include<bits/stdc++.h>
using namespace std;

int main() {
    vector<int> v;

    cout << "Initial capacity: " << v.capacity() << endl;

    v.push_back(10);
    cout << "Capacity after 1 push: " << v.capacity() << endl;

    v.push_back(10);
    cout << "Capacity after 2 pushes: " << v.capacity() << endl;

    return 0;
}
