#include <bits/stdc++.h>
using namespace std;
int sz = 0;
class Node
{

public:
    int val;
    Node *next;
    Node(int val)
    {
        this->val = val;
        this->next = NULL;
    }
};

void insert_tail(Node *head, Node *tail, int val)
{
    Node *newnode = new Node(val);

    if (head == NULL)
    {
        head = newnode;
        tail = newnode;
        return;
    }
    tail->next = newnode;
    tail = newnode;
}

int main()
{

    Node *head = NULL;
    Node *tail = NULL;
    int n;
    cin >> n;
    for (int i = 0; i < n; i++)
    {
        int val;
        cin >> val;
        insert_tail(head, tail, val);
    }
    int x;
    cin >> x;
    int idx = 0;
    bool flag = false;
    Node *tmp = head;
    while (tmp != NULL)
    {
        if (tmp->val == x)
        {
            cout << idx << " ";
            flag = true;
            break;
        }
        tmp = tmp->next;
        idx++;
    }
    if (flag == false)
    {
        cout << -1 << endl;
    }

    return 0;
}
