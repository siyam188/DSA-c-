
#include<bits/stdc++.h>
using namespace std;
int main()
{
   int N;
    cin >> N;

    vector<long long> A(N), prefix(N);

    for (int i = 0; i < N; i++) {
        cin >> A[i];
    }

    prefix[0] = A[0];
    for (int i = 1; i < N; i++) {
        prefix[i] = prefix[i - 1] + A[i];
    }

    for (int i = 0; i<n; i++) {
        cout << prefix[i] << " ";
    }
    return 0;
}
